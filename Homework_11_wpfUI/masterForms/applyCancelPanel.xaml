<UserControl x:Class="Homework_11_wpfUI.masterForms.applyCancelPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Homework_11_wpfUI.masterForms"
             mc:Ignorable="d">
    <DockPanel>
        <Button Margin="3,6,3,6" Content="Create" Width="100" Height="25" x:Name="applyBtn" Click="createBtn_Click"/>
        <Button Margin="3,6,3,6" Content="Cancel" Width="100" Height="25" x:Name="cancelBtn" Click="cancelBtn_Click"/>
    </DockPanel>
</UserControl>
